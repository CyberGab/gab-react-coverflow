{
  "name": "@cybergab/gab-react-coverflow",
  "version": "0.2.24",
  "description": "Carousel of coverflow effect",
  "main": "main.js",
  "homepage": "https://github.com/CyberGab/gab-react-coverflow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberGab/gab-react-coverflow.git"
  },
  "scripts": {
    "test": "make test",
    "prepublishOnly": "rm -rf dist && webpack --progress --colors -d",
    "clean": "rm -rf dist && rm -rf site/js/*.bundle.js",
    "build-npm": "rm -rf dist && webpack --progress --colors -d --config webpack.prod.js",
    "build:watch": "webpack --progress --colors --watch",
    "lint": "eslint ./src/Coverflow.js eslint ./site/js/main.js",
    "build-site": "rm -rf site/js/*.bundle.js && NODE_ENV=production webpack --config site/webpack.prod.js",
    "dev": "rm -rf site/js/*.bundle.js && NODE_ENV=react-hot webpack-dev-server --config site/webpack.dev.js --progress --colors --inline --hot --port 3000 --host 0.0.0.0 --content-base site"
  },
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "from": "/path/to/src.sss",
    "to": "/path/to/dest.css",
    "plugins": {
      "postcss-plugin": {}
    }
  },
  "keywords": [
    "Carousel",
    "Coverflow",
    "Image slider",
    "3D",
    "React",
    "react-component"
  ],
  "author": "CyberGab",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.15.0",
    "eslint-utils": "^3.0.0",
    "loadash": "^1.0.0",
    "mixin-deep": "^2.0.1",
    "radium": "^0.26.2",
    "websocket-extensions": "^0.1.4"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^6.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "glob": "^13.0.0",
    "html-webpack-plugin": "^5.6.5",
    "mocha": "^11.7.5",
    "node-sass": "^9.0.0",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-loader": "^4.13.1",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "bugs": {
    "url": "https://github.com/CyberGab/gab-react-coverflow/issues"
  },
  "directories": {
    "test": "test"
  }
}
